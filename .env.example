# Convex
CONVEX_DEPLOYMENT=dev:your-deployment
NEXT_PUBLIC_CONVEX_URL=https://your-deployment.convex.cloud

# Clerk Auth
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_xxx
CLERK_SECRET_KEY=sk_test_xxx
CLERK_JWT_ISSUER_DOMAIN=https://your-app.clerk.accounts.dev

# Inngest
# From Inngest Dashboard: signing key for request verification/signing (not a URL)
INNGEST_SIGNING_KEY=signkey-signing-xxx
# From Inngest Dashboard: event key for event authorization
INNGEST_EVENT_KEY=signkey-event-xxx

# Vercel Blob (set in Vercel dashboard; do not commit)
# BLOB_READ_WRITE_TOKEN=vercel_blob_rw_xxx

# Vercel OIDC (auto-injected by Vercel; do not commit)
# VERCEL_OIDC_TOKEN=xxx
